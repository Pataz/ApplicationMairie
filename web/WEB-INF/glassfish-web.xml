    <group-name>Greffier</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Procureur</role-name>
    <group-name>Procureur</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Substitut</role-name>
    <group-name>Substitut</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>PresidentChambre</role-name>
